 pipeline {
        agent any
        stages {
            
            stage('Checkout Code') {
            steps {
                git url: 'https://github.com/karthikvijay139/maven_sonar.git', branch: 'master'
            }
        }
         
          stage("build & sonarqube") {
            steps {
              withSonarQubeEnv('SonarQube_server') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }   
